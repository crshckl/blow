{
  "manifest_version": 3,
  "name": "Productivity Assistant",
  "description": "Manage tasks and get AI-powered suggestions for productivity.",
  "version": "1.0",
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/blow16.png",
      "48": "icons/blow48.png",
      "128": "icons/blow128.png"
    }
  }
}

